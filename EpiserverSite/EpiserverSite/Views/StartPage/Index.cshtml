@using EPiServer.Web.Mvc.Html

@model EpiserverSite.Models.ViewModels.StartPageViewModel

<div>
    @Html.LabelFor(m => m.CurrentPage.Robots)<br />
    @Html.PropertyFor(m => m.CurrentPage.Robots)<br />

    @Html.LabelFor(m => m.CurrentPage.OpenGraphTitle)<br />
    @Html.PropertyFor(m => m.CurrentPage.OpenGraphTitle)<br />

    @Html.LabelFor(m => m.CurrentPage.OpenGraphMedia)<br />
